<%- include dashboard-header %>
    <div class="row">
      <div class="col-md-4 col-md-offset-4">
        <h1>Events</h1>
      </div>
    </div>

    <div class="row">
      <% events.forEach(function(event){ %>
      
        <h4><a href="/dashboard/<%= orgId %>/event/<%= event._id %>"><%= event.name %></a></h4>

      <% }) %>
    </div>
<%- include dashboard-footer %>
